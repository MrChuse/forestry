{
  "@Centered": {
    "misc": {
      "text_horiz_alignment": "center"
    }
  },
  "@Font": {
    "font": {
      "name": "fira_code",
      "size": 20
    }
  },
  "@SmallFont": {
    "font": {
      "name": "fira_code",
      "size": 14
    }
  },
  "@TooltipDelay": {
    "misc": {
      "tool_tip_delay": "0.4"
    }
  },
  "@Dominant": {
    "colours": {
      "normal_text": "#ec3661"
    }
  },
  "@Recessive": {
    "colours": {
      "normal_text": "#3687ec"
    }
  },
  "#BeeButton": {
    "prototype": "@TooltipDelay"
